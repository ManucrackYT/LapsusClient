  },
  "defaulttheme": "lapsus",
  "timezone": "Europe/London",
  "main_web": "https://yourdomain.com",
  "discord_server": "https://discord.gg/invite/",
  
  "profile_json": true,

  "news": {
    "new1_type": "Recommendation",
