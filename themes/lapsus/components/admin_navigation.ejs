          <div class="flex items-center">
            <div class="ml-2">
              <p class="text-xs font-medium text-white"><%= coins.toLocaleString(undefined, {maximumFractionDigits:0}) %> coin<%= coins == 1 ? "" : "s" %> in your account </p>
            </div>
          </div>
        </a>
        <script>
// Select the element that you want to track clicks on
const myElement = document.getElementById('coins');

// Set a click counter to 0
let clickCounter = 0;

// Add a click event listener to the element
myElement.addEventListener('click', function() {
  // Increase the click counter by 1
  clickCounter++;

  // Check if the click counter is equal to 5
  if (clickCounter === 5) {
    // Redirect the user to the desired URL
    window.location.href = '../coins';
  }
});

// Function to copy text to clipboard
function copyToClipboard(text) {
    navigator.clipboard.writeText(text).then(() => {
      // Show success message using alertify
      alertify.success('Copied ID: ' + text);
    }).catch(err => {
      // Show error message if copying fails
      alertify.error('Failed to copy ID: ' + err);
    });
  }

  // Add click event listener to the username element
  document.addEventListener('DOMContentLoaded', function() {
    const usernameElement = document.querySelector('.flex-shrink-0.w-full.group.block .text-sm.font-medium.text-white');
    if (usernameElement) {
      usernameElement.addEventListener('click', function() {
        // Get the user ID from the EJS variable
        const userId = '<%= userinfo.id %>';
        copyToClipboard(userId);
      });
    }
  });
        </script>
      </div>
    </div>
  </div>